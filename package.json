{
  "name": "vouchsafe-client",
  "version": "0.1.0",
  "description": "Official Vouchsafe SDK for Node.js and TypeScript",
  "author": "Vouchsafe",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "clean:sdk": "rm -rf src/openapi",
    "generate": "npx openapi-generator-cli generate -i https://app.vouchsafe.id/openapi/swagger.json -g typescript-fetch -o src/openapi --skip-validate-spec   --additional-properties=modelPropertyNaming=original",
    "prepare": "npm run build"
  },
  "keywords": [
    "vouchsafe",
    "kyc",
    "aml",
    "identity",
    "verification",
    "sdk",
    "typescript",
    "node"
  ],
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.22.0",
    "typescript": "^5.9.2"
  }
}
